{
  "name": "minireact",
  "version": "0.1.0",
  "description": "A minimal react project boilerplate",
  "main": "src/index.ts",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack",
    "build:analyze": "WEBPACK_USE_ANALYZE=1 webpack",
    "build:dev": "APP_ENV=dev NODE_ENV=production webpack",
    "build:prod": "APP_ENV=prod NODE_ENV=production webpack",
