    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tabler-icons-react": "^1.56.0",
    "ts-loader": "^9.4.2"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "core-js": "^3.30.0",
    "css-loader": "^6.7.3",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.0",
